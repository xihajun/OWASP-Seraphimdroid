<resources>
    <!-- Strings for compressing a database -->
    <string name="compress_confirm_title">Compress the database?</string>
    <string name="compress_confirm_message">This will merge consecutive rows with identical data.</string>

    <string name="compress_toast_start">Compressing the database</string>

    <string name="compress_feature_name">Database compression</string>

    <string name="compress_notif_progress_title">Database compression</string>
    <string name="compress_notif_progress_content">Processing %1$d of %2$d</string>
    <string name="compress_notif_complete_title">Database compressed</string>
    <plurals name="compress_notif_complete_content">
        <item quantity="one">%1$d row deleted from the database</item>
        <item quantity="other">%1$d rows deleted from the database.</item>
    </plurals>
    <string name="compress_notif_canceled_content">The database compression was canceled.</string>
    <string name="compress_notif_error_content">Failed to compress the database.</string>
</resources>